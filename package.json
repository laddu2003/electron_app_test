{
  "name": "pingo",
  "version": "1.0.9",
  "description": "Love for NodeJS",
  "main": "src/core/main.js",
  "private": "true",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "release": "build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laddu2003/electron_app_test.git"
  },
