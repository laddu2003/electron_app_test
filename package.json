  "devDependencies": {
    "electron": "^8.2.4",
    "electron-builder": "^22.3.2"
  },
  "build": {
    "appId": "com.applyhead.pingo",
    "productName": "Pingo",
    "copyright": "Copyright Â© year laddu corp",
    "win": {
      "target": [
        "NSIS"
      ]
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "publish": [
      {
        "provider": "github"
      }
    ]
  },
  "dependencies": {
    "electron-updater": "^4.0.4"
  },
  "bugs": {
